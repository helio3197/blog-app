<section>
  <div class="users-list">
    <%= render @user %>
  </div>
  <div class="users-posts">
    <% @posts.each do |post| %>
      <a href=<%= user_post_path(@user, post) %> class="post">
        <%= render post %>
        <%= render partial: 'comments', object: post.last_five_comments %>
      </a>
    <% end %>
  </div>
</section>
